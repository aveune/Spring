<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0 //EN" 
                             "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.spring.k.user.UserVo"  alias="UserVo" />
		<typeAlias type="com.spring.k.board.BoardVo"  alias="BoardVo" />
		<typeAlias type="com.spring.k.reply.ReplyVo"  alias="ReplyVo" />

    </typeAliases>
	
	<mappers>
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/board-mapping.xml" />
		<mapper resource="mappings/reply-mapping.xml" />
	
	</mappers>	

</configuration>